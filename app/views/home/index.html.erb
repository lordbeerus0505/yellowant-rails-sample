<div class="container">
	<div class="row justify-content-center">
		<div >
			<br> <h3>Welcome to New App</h3> <br>
			<div class="row justify-content-center">
				<%= button_to "Integrate", { :action => "red_yellowant"}, {:method => :get, }%> <br>
			</div>
		</div>
	</div>
	<div class="row justify-content-center">
		<div>
			<% if @accounts %>
				<br><ul class="list-group">
				<% @accounts.each do |account| %>
